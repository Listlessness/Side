{
  "expo": {
    "name": "Side Stream",
    "slug": "Side-Stream",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.reidnsc.Side"
    },
    "androidNavigationBar": {
      "backgroundColor": "#000000"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Anime streaming app. A side project. Retrieves content from third party sources (MyAnimeList, Streamani.net, and GoGoAnime); and makes them available to you in a user-friendly interface.",
    "githubUrl": "https://github.com/Neko-R/Side"
  }
}
